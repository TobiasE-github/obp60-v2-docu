#: ../../usermanual/configuration.rst:707 eed74b17e7144d43abf0680827ce2003
#, fuzzy
msgid ""
"a=1-(0.3 + ((k - 0.01) * (0.95 - 0.3) / (10 - 0.01)))\n"
"\n"
msgstr ""
"a=1-(0.3 + ((k - 0.01) * (0.95 - 0.3) / (10 - 0.01)))\n"
"\n"

#: ../../usermanual/configuration.rst:709 a3362dcf71b1479aae526b2474a5ed20
